cmake_minimum_required(VERSION 3.0.2)
project(twoD_to_pcl)

## Compile as C++11, supported in ROS Kinetic and newer
##dd_compile_options(-std=c++11)

find_package(catkin REQUIRED
  geometry_msgs
  message_generation
  roscpp
  std_msgs
  sensor_msgs
  visualization_msgs
  pcl_ros
  pcl_conversions
  laser_geometry

)



## Generate messages in the 'msg' folder
add_message_files(
  FILES
)


## Generate added messages and services with any dependencies listed here
generate_messages(
  DEPENDENCIES
)


catkin_package(
#  INCLUDE_DIRS include
#  LIBRARIES static_small
#  CATKIN_DEPENDS ackermann_msgs actionlib actionlib_msgs geometry_msgs message_generation message_runtime obstacle_detector roscpp rospy sensor_msgs visualization_msgs
#  DEPENDS system_lib
)


include_directories(
# include
  ${catkin_INCLUDE_DIRS}
)


add_executable(${PROJECT_NAME}_node src/twoD_to_pcl.cpp) 

target_link_libraries(${PROJECT_NAME}_node 
${catkin_LIBRARIES})

add_dependencies(${PROJECT_NAME}_node ${${PROJECT_NAME}_EXPORTED_TARGETS} ${catkin_EXPORTED_TARGETS})

